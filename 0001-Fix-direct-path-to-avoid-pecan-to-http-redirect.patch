Subject: [PATCH] Fix direct path to avoid pecan to http redirect

Change-Id: Icd1cc132582eace9cb47085813016a681e0fb958
---
 public/javascript/repoxplorer.js | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/public/javascript/repoxplorer.js b/public/javascript/repoxplorer.js
index 7642dec..dc4e8d7 100644
--- a/public/javascript/repoxplorer.js
+++ b/public/javascript/repoxplorer.js
@@ -45,7 +45,7 @@ function get_groups(nameonly, prefix) {
     var args = {};
     args['nameonly'] = nameonly;
     args['prefix'] = prefix;
-    return $.getJSON("api/v1/groups", args);
+    return $.getJSON("api/v1/groups/", args);
 }
 
 function fill_status() {
@@ -194,7 +194,7 @@ function get_infos(pid, tid, cid, gid) {
         gc_d = $.getJSON("api/v1/infos/contributor", {'cid': cid});
     }
     if (gid) {
-        gg_d = $.getJSON("api/v1/groups", args);
+        gg_d = $.getJSON("api/v1/groups/", args);
     }
 
     gi_d = $.getJSON("api/v1/infos/infos", args);
